<!DOCTYPE html>
<html>
  <head>
    <title>DisOrder</title>
    <%= csrf_meta_tags %>
    
    <script type="text/javascript">
      window.homeIconURL = "<%= image_url('default-icon-no-bg.png') %>";
      window.tempIcon = "<%= image_url('red-default-icon.png') %>";
      window.editIcon = "<%= image_url('please-edit-message.png') %>%";
    </script>
    <script type="text/javascript">
      window.logoutUrl = "<%= image_url('logout-icon.png') %>";
    </script>
    <%= favicon_link_tag  asset_path('favicon.ico')  %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
